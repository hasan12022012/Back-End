@model HomeVM

@{
    ViewData["Title"] = "Home Page";
}

@section Links{
    <link rel="stylesheet" href="~/assets/slick/slick.css" />
    <link rel="stylesheet" href="~/assets/slick/slick-theme.css" />
    <link rel="stylesheet" href="~/assets/css/responsive.css" />
}

<section class="imageslider">
    <div class="container">
        <div class="slider">
            <div class="slides">
                <input type="radio" name="radio-btn" id="radio1">
                <input type="radio" name="radio-btn" id="radio2">
                <input type="radio" name="radio-btn" id="radio3">
                @foreach (var slider in Model.Sliders)
                {
                    <div class="slide">
                        <img src="~/assets/img/@slider.Image" alt="">
                        <div class="slide-text">
                            <span>@slider.Title</span>
                            <h1 class="img-slide-title">@Html.Raw(slider.Content)</h1>
                            <a asp-controller="Product" asp-action="Index">
                                <button class="browse-store-btn">
                                    <h1>Browse Store</h1>
                                </button>
                            </a>
                        </div>
                    </div>
                }

                <div class="navigation-auto">
                    <div class="auto-btn1"></div>
                    <div class="auto-btn2"></div>
                    <div class="auto-btn3"></div>
                </div>

            </div>
            <div class="navigation-manual">
                <label for="radio1" class="manual-btn"></label>
                <label for="radio2" class="manual-btn"></label>
                <label for="radio3" class="manual-btn"></label>
            </div>
        </div>
    </div>
</section>

<section class="bestsellingbooksever">
    <div class="container">
        <h2>Best Selling Books Ever</h2>
        <div class="wrapper">
            <i id="left" class="fa-solid fa-angle-left"></i>
            <ul class="carousel">
                @foreach (var product in Model.Products)
                {
                    <li class="card">
                        <div class="card_image">
                            <a asp-controller="Product" asp-action="Detail" asp-route-id="@product.Id"><img src="~/assets/img/@product.ProductImages.FirstOrDefault(m => m.IsMain).Name" alt=""></a>
                        </div>
                        <div class="card_content">
                            <a asp-controller="Product" asp-action="Detail" asp-route-id="@product.Id">@product.Name</a>
                            <p>@product.Author.Name</p>
                            <div class="card_text">
                                <div class="starsreview">
                                    <i class="fa-solid fa-star"></i>
                                    <i class="fa-solid fa-star"></i>
                                    <i class="fa-solid fa-star"></i>
                                    <i class="fa-solid fa-star"></i>
                                    <i class="fa-solid fa-star-half-stroke"></i>
                                </div>
                                <div class="price">
                                    <span>$@product.Price</span>
                                </div>
                            </div>
                        </div>
                    </li>
                }
            </ul>
            <i id="right" class="fa-solid fa-angle-right"></i>
        </div>
    </div>
</section>

